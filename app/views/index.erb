<h1>All Robots</h1>

<ol>
  <% @robots.each do |robot|%>
  <li><a href="/robots/<%= robot.id %>"><%= robot.name %></a>
  <a href="/robots/<%= robot.id %>/edit">Edit</a>
  <form class="" action="/robots/<%= robot.id %>" method="post">
    <input type="hidden" name="_method" value="DELETE">
    <input type="submit" value="Delete">
  </form>
</li>
 <% end %>
 </ol>
